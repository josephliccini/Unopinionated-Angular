  "description": "Unopinionated Angular Toolbox",
  "main": "./dist/lib/index.js",
  "module": "./dist/lib/index.js",
  "typings": "./dist/lib/index.d.ts",
  "angular-cli": {},
  "scripts": {
    "start": "webpack-dev-server --open --hot", 
    "lint": "tslint \"src/**/*.ts\"",
    "pree2e": "webdriver-manager update",
    "e2e": "protractor",
    "build": "cd src/lib && ngc -p tsconfig-aot.json && cd .. && cd .. && gulp mv:templates && gulp inline-resources",
    "bundle": "cd dist/lib && rollup -c",
    "publish": "npm run build && npm run bundle",
    "buildwp": "webpack --progress",
    "demo": "webpack-dev-server --open --hot",
    "buildDemoAot": "cd src/demo && ngc -p tsconfig-aot.json && cd .. && cd .."
  },
  "private": false,
  "dependencies": {
