{
  "name": "unopinionated-angular-toolbox",
  "version": "0.1.0",
  "author": "Thomas Elliott",
  "license": "MIT",
  "description": "Unopinionated Angular Toolbox",
  "private": false,
  "main": "./dist/lib/index.js",
  "module": "./dist/lib/index.js",
  "typings": "./dist/lib/index.d.ts",
  "angular-cli": {},
  "scripts": {
    "start": "npm run build && npm run demo",
    "lint": "tslint \"src/**/*.ts\"",
    "pree2e": "webdriver-manager update",
    "e2e": "protractor",
    "build": "cd src/lib && ngc -p tsconfig-aot.json && cd .. && cd .. && gulp mv:templates && gulp inline-resources",
    "bundle": "cd dist/lib && rollup -c",
    "publish": "npm run build && npm run bundle",
    "buildwp": "webpack --progress",
    "demo": "webpack-dev-server --open --hot",
    "clean": "rm -r ./dist && rm -r ./docs",
    "buildDemoAot": "cd src/demo && ngc -p tsconfig-aot.json && cd .. && cd ..",
    "genDoc": "cd src/lib && typedoc --out ../../docs --exclude \"**/+(*.spec.ts|*.css|*.*css|*.html|*.config.ts)\" --excludeExternals",
    "fixDoc": "gulp rename:md"
  },
  "dependencies": {
    "@angular/animations": "^4.0.0",
    "@angular/common": "^4.0.0",
    "@angular/compiler": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@angular/forms": "^4.0.0",
    "@angular/http": "^4.0.0",
    "@angular/platform-browser": "^4.0.0",
    "@angular/platform-browser-dynamic": "^4.0.0",
    "@angular/router": "^4.0.0",
    "core-js": "^2.4.1",
    "rxjs": "^5.1.0",
    "zone.js": "^0.8.4",
    "ts-helpers": "^1.1.1"
  },
  "devDependencies": {
    "@angular/cli": "1.2.0",
    "@angular/compiler-cli": "^4.0.0",
    "@angularclass/hmr": "latest",
    "@angularclass/hmr-loader": "latest",
    "@ngtools/webpack": "latest",
    "@types/jasmine": "^2.2.30",
    "@types/node": "^6.0.42",
    "angular2-template-loader": "latest",
    "autoprefixer": "^6.5.3",
    "awesome-typescript-loader": "latest",
    "codelyzer": "latest",
    "css-loader": "latest",
    "cssnano": "^3.10.0",
    "extract-text-webpack-plugin": "latest",
    "file-loader": "latest",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.1",
    "gulp-clean": "^0.3.2",
    "gulp-clean-css": "^3.0.0",
    "gulp-cli": "^1.2.2",
    "gulp-htmlmin": "^3.0.0",
    "gulp-if": "^2.0.2",
    "gulp-markdown": "^1.2.0",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^2.3.2",
    "gulp-shell": "^0.5.2",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-transform": "^1.1.0",
    "gulp-typedoc": "^2.0.2",
    "gulp-typescript": "^3.2.0",
    "html-webpack-plugin": "latest",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-router-loader": "latest",
    "node-sass": "^4.5.3",
    "postcss-url": "^5.1.2",
    "protractor": "~5.1.2",
    "raw-loader": "latest",
    "rimraf": "^2.5.4",
    "rollup": "^0.37.0",
    "string-replace-loader": "latest",
    "to-string-loader": "latest",
    "ts-node": "latest",
    "tslint": "latest",
    "typedoc": "^0.7.1",
    "typedoc-md-theme": "^1.0.1",
    "typescript": "~2.3.3",
    "webdriver-manager": "10.2.5",
    "webpack": "^2.2.0",
    "webpack-dev-server": "^2.2.0"
  }
}
