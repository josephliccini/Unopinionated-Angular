    "@compodoc/compodoc": "^1.0.0-beta.15",
    "@ngtools/webpack": "latest",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "angular2-template-loader": "latest",
    "autoprefixer": "^6.5.3",
    "awesome-typescript-loader": "latest",
    "circular-dependency-plugin": "^3.0.0",
