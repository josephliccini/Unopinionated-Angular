{
  "name": "unopinionated-angular-toolbox",
  "version": "0.1.1",
  "author": "Thomas Elliott",
  "license": "MIT",
  "description": "Unopinionated Angular Toolbox",
  "private": false,
  "main": "./dist/lib/index.js",
  "module": "./dist/lib/index.js",
  "typings": "./dist/lib/index.d.ts",
  "angular-cli": {},
  "scripts": {
    "start": "npm run build && npm run demo",
    "lint": "tslint \"src/**/*.ts\"",
    "pree2e": "webdriver-manager update",
    "e2e": "protractor",
    "build": "cd src/lib && ngc -p tsconfig.json && cd .. && cd .. && gulp mv:templates && gulp inline-resources",
    "bundle": "cd dist/lib && rollup -c",
    "prepublish": "npm run build && npm run bundle & npm run genDoc && touch ./docs/.nojekyll ",
    "demo": "webpack-dev-server --open --hot --config \"./configs/webpack/webpack.config.dev.js\"",
    "clean": "rm -r ./dist && rm -r ./docs",
    "bd": "npm run buildDemo",
    "buildDemo": "webpack --config \"./configs/webpack/webpack.config.prod.js\" --progress",
    "serve": "lite-server -c \"./configs/lite-server/lite-server.config.json\"",
    "genDoc": "./node_modules/.bin/compodoc -p src/lib/tsconfig.json -d \"./docs\" "
  },
  "dependencies": {
    "@angular/animations": "^4.2.4",
    "@angular/common": "^4.2.4",
    "@angular/compiler": "^4.2.4",
    "@angular/core": "^4.2.4",
